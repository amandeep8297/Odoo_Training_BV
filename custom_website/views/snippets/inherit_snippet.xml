<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="external_snippets" inherit_id="website.snippets" name="Custom Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">Brainvire</div>
                <div class="o_panel_body">
                    <t t-snippet="custom_website.basic_snippet" t-thumbnail="custom_website/static/src/img/bv.png"/>
                    <t t-snippet="custom_website.basic_snippet2" t-thumbnail="custom_website/static/src/img/snippets/ums.webp"/>
                     <t t-snippet="custom_website.basic_snippet3"/>
                     <t t-snippet="custom_website.slider_snippet"/>
                </div>
            </div>
        </xpath>
    </template>
</odoo>
